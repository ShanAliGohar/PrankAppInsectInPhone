<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="8dp"
    android:background="@drawable/rounded_bg"
    android:backgroundTint="#FFC964"
    android:padding="16dp">

    <!-- Top view with match_parent width and 100dp height -->
    <View
        android:id="@+id/top_view"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/rounded_bg"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="16dp" />


    <View
        android:id="@+id/view_one"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/rounded_bg"
        android:backgroundTint="#FE830A"
        app:layout_constraintBottom_toBottomOf="@+id/top_view"
        app:layout_constraintEnd_toStartOf="@id/view_two"
        app:layout_constraintStart_toStartOf="@+id/top_view"
        app:layout_constraintTop_toTopOf="@+id/top_view"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/view_two"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"

        android:layout_marginBottom="8dp"
        android:background="@drawable/rounded_bg"
        android:backgroundTint="#FE830A"
        app:layout_constraintBottom_toBottomOf="@+id/top_view"
        app:layout_constraintEnd_toStartOf="@id/view_three"
        app:layout_constraintStart_toEndOf="@+id/view_one"
        app:layout_constraintTop_toTopOf="@+id/top_view" />

    <View
        android:id="@+id/view_three"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/rounded_bg"
        android:backgroundTint="#FE830A"
        app:layout_constraintBottom_toBottomOf="@+id/top_view"
        app:layout_constraintEnd_toStartOf="@id/view_four"
        app:layout_constraintStart_toEndOf="@+id/view_two"
        app:layout_constraintTop_toTopOf="@+id/top_view" />

    <View
        android:id="@+id/view_four"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/rounded_bg"
        android:backgroundTint="#FE830A"
        app:layout_constraintBottom_toBottomOf="@+id/top_view"
        app:layout_constraintEnd_toStartOf="@id/view_five"
        app:layout_constraintStart_toEndOf="@+id/view_three"
        app:layout_constraintTop_toTopOf="@+id/top_view" />

    <View
        android:id="@+id/view_five"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/rounded_bg"
        android:backgroundTint="#FE830A"
        app:layout_constraintBottom_toBottomOf="@+id/top_view"
        app:layout_constraintEnd_toEndOf="@+id/top_view"
        app:layout_constraintStart_toEndOf="@+id/view_four"
        app:layout_constraintTop_toTopOf="@+id/top_view" />

    <TextView
        android:id="@+id/text_one"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:fontFamily="@font/elsie_black"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_one"
        app:layout_constraintEnd_toEndOf="@+id/view_one"
        app:layout_constraintStart_toStartOf="@+id/view_one"
        app:layout_constraintTop_toTopOf="@+id/view_one" />

    <TextView
        android:id="@+id/text_two"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/elsie_black"
        android:text="2"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/text_one"
        app:layout_constraintEnd_toEndOf="@+id/view_two"
        app:layout_constraintStart_toStartOf="@+id/view_two"
        app:layout_constraintTop_toTopOf="@+id/text_one" />

    <TextView
        android:id="@+id/text_three"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/elsie_black"
        android:text="3"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_three"
        app:layout_constraintEnd_toEndOf="@+id/view_three"
        app:layout_constraintStart_toStartOf="@+id/view_three"
        app:layout_constraintTop_toTopOf="@+id/view_three" />

    <TextView
        android:id="@+id/text_four"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/elsie_black"
        android:text="4"
        android:textSize="25dp"

        app:layout_constraintBottom_toBottomOf="@+id/text_three"
        app:layout_constraintEnd_toEndOf="@+id/view_four"
        app:layout_constraintStart_toStartOf="@+id/view_four"
        app:layout_constraintTop_toTopOf="@+id/text_three" />

    <TextView
        android:id="@+id/text_five"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/elsie_black"
        android:text="5"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/text_four"
        app:layout_constraintEnd_toEndOf="@+id/view_five"
        app:layout_constraintStart_toStartOf="@+id/view_five"
        app:layout_constraintTop_toTopOf="@+id/text_four" />
    <View
        android:id="@+id/bottom_view"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/rounded_bg"
        android:backgroundTint="#FF7E00"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/top_view"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Start"
        android:fontFamily="@font/elsie_black"
        android:textSize="25dp"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/bottom_view"
        app:layout_constraintEnd_toEndOf="@+id/bottom_view"
        app:layout_constraintStart_toStartOf="@+id/bottom_view"
        app:layout_constraintTop_toTopOf="@+id/bottom_view" />
</androidx.constraintlayout.widget.ConstraintLayout>